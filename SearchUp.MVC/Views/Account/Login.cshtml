@model Application.ViewModels.LoginViewModel

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Join SearchUp</title>
    <link rel="stylesheet" href="/css/style.css">
</head>
<body>
    <header class="header">
        <div class="header_inner">
            <a class="logo" asp-controller="Home" asp-action="Index">SearchUp</a>
        </div>
    </header>
    <div class="content">
        <div class="sighUpForm">
            <form method="post">
                <p>Email</p>
                <input type="email">
                <p>Password</p>
                <input type="password">
                <p><input type="submit" value="Sign up"></p>
            </form>
        </div>
    </div>
    <footer class="footer">
        <div class="footer_inner">
            <a class="nav_link" href="#">About us</a>
            <a class="nav_link" href="#">About project</a>
        </div>
    </footer>
</body>
</html>