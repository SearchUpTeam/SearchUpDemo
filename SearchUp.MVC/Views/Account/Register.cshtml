@model Application.ViewModels.RegisterViewModel
    <!DOCTYPE html>
    <html lang="en">
    <head>
        <meta charset="UTF-8">
        <title>Join SearchUp</title>
        <link rel="stylesheet" href="/css/style.css">
    </head>
    <body>
        <header class="header">
            <div class="header_inner">
                <a class="logo" asp-controller="Home" asp-action="Index">SearchUp</a>
            </div>
        </header>
        <div class="content">
            <div class="sighUpForm">
                <form method="post" asp-controller="Account" asp-action="Register">
                    <p>Email</p>
                    <input asp-for="Email" type="email">
                    <p>BirthDate</p>
                    <input asp-for="BirthDate" type="date">
                    <p>Password</p>
                    <input asp-for="Password" type="password">
                    <p>Confirm password</p>
                    <input asp-for="PasswordConfirm" type ="password">
                    <p><input type="submit" value="Sign up"></p>
                </form>
            </div>
        </div>
        <footer class="footer">
            <div class="footer_inner">
                <a class="nav_link" href="#">About us</a>
                <a class="nav_link" href="#">About project</a>
            </div>
        </footer>
    </body>
</html>